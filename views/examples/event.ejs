          <div class="card">
            <!-- Card header -->
            <div class="card-header border-0">
              <h3 class="mb-0">Event</h3>
            </div>
            <div class="table-responsive">
              <table class="table align-items-center table-flush">
                <thead class="thead-light">
                  <tr>
                    <th scope="col" class="sort" data-sort="id">Id</th>
                    <th scope="col" class="sort" data-sort="name">Name</th>
                    <th scope="col" class="sort" data-sort="date">Date</th>
                    <th scope="col" class="sort" data-sort="position">Position</th>
                    <th scope="col" class="sort" data-sort="position"></th>
                  </tr>
                </thead>

                <% eventList.forEach((event) => { %>
                <tbody class="list">
                  <tr>
                    <th scope="row">
                      <div class="media align-items-center">
                        <div class="media-body">
                          <span name="id" class="name mb-0 text-sm"><%= event.id %></span>
                        </div>
                      </div>
                    </th>
                    <td>
                      <span class="badge badge-dot mr-4">
                        <span class="name"><%= event.name %></span>
                      </span>
                    </td>
                    <td>
                      <span class="badge badge-dot mr-4">
                        <span class="date"><%= event.date %></span>
                      </span>
                    </td>
                    <td>
                      <span class="badge badge-dot mr-4">
                        <span class="position"><%= event.position %></span>
                      </span>
                    </td>
                    <td class="text-right">
                      <div class="dropdown">
                        <a class="btn btn-sm btn-icon-only text-light" href="#" role="button" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
                          <i class="fas fa-ellipsis-v"></i>
                        </a>
                        <div class="dropdown-menu dropdown-menu-right dropdown-menu-arrow">
                          <form action="/admin/updateEventElement" class="event-form">
                            <input type="text" name="searchId" placeholder="id" value="<%= event.id %>" style="display: none;">
                            <button type="submit" class="dropdown-item">Update</button>
                          </form>
                          <form action="/admin/deleteEvent" class="search-form">
                            <input type="text" name="deleteEvent" value="<%= event.id %>" style="display: none;">
                            <button type="submit" class="dropdown-item">Delete</button>
                          </form>
                        </div>
                      </div>
                    </td>
                  </tr>
                  <% }) %>
                </tbody>
              </table>
            </div>
              </table>
